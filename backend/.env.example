# Soniox live transcript API key
SONIOX_API_KEY=your_soniox_api_key_here

# Database
DATABASE_URL=postgresql+psycopg2://user:password@host:5432/dbname

# Redis
REDIS_URL=rediss://:password@host:6380/0

# Azure OpenAI
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/openai/deployments/gpt-4o/chat/completions?api-version=2025-01-01-preview
AZURE_OPENAI_API_KEY=your_azure_openai_key
AZURE_OPENAI_DEPLOYMENT=gpt-4o

# Azure OpenAI Mini
AZURE_OPENAI_ENDPOINT_mini=https://your-resource.openai.azure.com/openai/deployments/gpt-4.1-mini/chat/completions?api-version=2025-01-01-preview
AZURE_OPENAI_API_KEY_mini=your_azure_openai_key
AZURE_OPENAI_DEPLOYMENT_mini=gpt-4.1-mini

# Murf TTS
MURF_API_KEY=your_murf_api_key

# Twilio SMS (optional)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
